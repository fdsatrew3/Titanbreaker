<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="file://{resources}/styles/custom_game/auto_casts_config_window.css" />
	</styles>
	<scripts>
        <include src="file://{resources}/scripts/custom_game/auto_casts_config_window.js" />
	</scripts>
	<Panel hittest="false" class="AutoCastsConfigWindowContainer Hidden">
		<Label text="Amount of Resource to preserve" class="AutoCastsConfigWindowLabel"/>
